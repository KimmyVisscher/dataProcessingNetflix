						"type": "string"
					},
					"episode_duration": {
						"type": "integer"
					}
				}
			},
			"Series": {
				"type": "object",
				"properties": {
					"id": {
						"type": "integer"
					},
					"name": {
						"type": "string"
					},
					"episodes": {
						"type": "array",
						"items": {
							"$ref": "#/components/schemas/Episode"
						}
					}
				}
			},
			"Watchlist": {
				"type": "object",
				"properties": {
					"watchlist_items": {
						"type": "array",
						"items": {
							"oneOf": [
								{"$ref": "#/components/schemas/Movie"},
								{"$ref": "#/components/schemas/Episode"}
							]
						}
					}
				}
			},
			"Account": {
				"properties": {
					"id": {
						"type": "integer"
					},
					"email": {
						"type": "string"
					},
					"is_blocked": {
						"type": "boolean"
					}
				}
			},
			"Profile": {
				"type": "object",
				"properties": {
					"id": {
						"type": "integer"
					},
					"name": {
						"type": "string"
					},
					"photo_path": {
						"type": "string"
					},
					"is_child": {
						"type": "boolean"
